  <script src="/javascripts/zepto.min.js"></script>
  <script>

  function getOffsetOfEle(startEle, stopEle){
    var pos = {
      left: 0,
      top: 0
    };
    while (startEle != stopEle) {
      pos.left += startEle.offsetLeft;
      pos.top += startEle.offsetTop;
      startEle = startEle.offsetParent;
    }

    return pos;
  }

    $(function () {
      var windowHeight = document.documentElement.clientHeight;
      $('.mask, .zui-sideBox, body').height(windowHeight);
      var height = windowHeight - 34;
      $('.zui-indexLists').height(height);
      var aHeight = (windowHeight - 15) / 26;
      $('.indexListsBar span').css({
        height: aHeight,
        lineHeight: aHeight +'px'
      })
    });

    $('.zui-sideBox').swipe(function(e){
     e.stopPropagation();
    });

    $('.zui-topBar .icon-navlist').tap(function(){
      $('.zui-sideBox').css({
        transition: 'all 0.3s linear'
      })
      $('.zui-sideBox').addClass('in');
    });

    $('.zui-sideBox .mask, .zui-sideBox .head-back').tap(function(e){
      e.stopPropagation();
      $('.zui-sideBox').removeClass('in');
    });

    var prevPageX = 0
    $('.zui-sideBox').on({
      touchstart: function (e) {
         prevPageX = e.touches[0].pageX;
      } ,
      touchmove: function(e){
        e.preventDefault();
        e.stopPropagation();
        var PageX = e.touches[0].pageX;
        var a = $(this).css('transform')
        var current = parseInt(a.slice(a.indexOf('(')+1, -1))

       
        $('.zui-sideBox')[0].style.transition = '';

        $(this).css({
          transform: 'translateX(-'+((  prevPageX - PageX) - current)+'px)'
        })
        
        prevPageX = PageX;
        
        }
    });

    $('body').swipe(function(e){
      e.preventDefault();
      e.stopPropagation();
    })

    $('.indexListsBar span').click(function(e){
      e.preventDefault();
      var target = $(this).data('target');
      var h3 = $('.zui-indexLists').find('h3[data-group = '+target+']')[0];
      var pos = getOffsetOfEle(h3, $('.zui-indexListsBox')[0]);
      $('.zui-indexLists ol').css({
        'transform': "translateY(-"+pos.top+"px)"
      })
    });

    // $('.view').swipeRight(function(e){
    //   $('.zui-sideBox').css({
    //     transition: 'all 0.3s linear'
    //   }).addClass('in');
    // });

    var indexListBar = document.querySelector('.indexListsBar');

    indexListBar.addEventListener('touchstart', function  (ev) {
      // console.log(222)
    })
    var timer = null;
    indexListBar.addEventListener('touchmove', function  (ev) {
      ev.stopPropagation();
      ev.preventDefault();
      clearTimeout(timer);
      timer = setTimeout(function(){
        var targetEle = ev.target;
        if(targetEle.tagName.toLowerCase() === 'span'){
          var index = Math.ceil((ev.touches[0].pageY - $(indexListBar).offset().top) / 23);
          var span = $('.indexListsBar span').eq(index);
          console.log(index)
          var h3 = $('.zui-indexLists').find('h3[data-group = '+$(span).data('target')+']')[0];
          if(h3){
          var pos = getOffsetOfEle(h3, $('.zui-indexListsBox')[0]);
          $('.zui-indexLists ol').css({
            'transform': "translateY(-"+pos.top+"px)"
          })
          }
        }
      }, 10);
    });
  </script>
  
